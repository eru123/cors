{
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "watch": "sass --watch ./src/assets:./public --no-source-map --style compressed",
    "build": "sass ./src/assets:./public --no-source-map --style compressed && minify ./src/assets/client.js > ./public/main.js",
    "start": "node ./src/index.js"
  },
  "devDependencies": {
    "minify": "^9.1.0",
    "nodemon": "^2.0.20",
    "sass": "^1.54.9"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "lodash": "^4.17.21"
  }
}
